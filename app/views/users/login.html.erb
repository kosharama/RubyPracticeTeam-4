<h1>Вход</h1>

<%= form_with(model: @user, url: login_path) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit 'Войти' %>
  </div>
<% end %>